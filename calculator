public class Calculator{

  public float plus(float a,float b){
  
  }
  public float minus(float a,float b){
  
  }
  public float mulitply(float a,float b){
    return a * b;
  }
  public float divide(float a,float b){
    return a / b;
  }
  public float min(float a,float b)
  {
    if(a>b)
    {
      return b; 
    }
    else
    {
      return a;
    }
  }
  public Calculator(){
  
  }

}
